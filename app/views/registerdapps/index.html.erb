<h1>Applications</h1>
<div style="margin-top:1.5em;">
    <%= link_to "New Application", new_registerdapp_path, class: 'btn btn-success' %>
</div>

<% @apps.each do |app| %>
	<div class="media">
     	<div class="media-body">
   			<h4 class="media-heading">
     			<%= link_to app.title, registerdapp_path(wiki) %>
   			</h4>
     	</div>
	</div>
<% end %>